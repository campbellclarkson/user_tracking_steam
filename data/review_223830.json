0438 \u043d\u0435\u0440\u0432\u044b \u043f\u043e\u0441\u043b\u0435 100 \u0441\u0431\u0438\u0442\u043e\u0433\u043e \u041d\u041b\u041e, \u044f \u0443\u0437\u043d\u0430\u043b \u0447\u0442\u043e \u043a\u043e\u043d\u0435\u0446 \u0438\u0433\u0440\u044b, \u0433\u0434\u0435-\u0442\u043e \u043d\u0430 250-\u043e\u043c. \u042d\u0442\u043e \u043f\u043e \u0441\u0443\u0442\u0438 \u0438 \u043d\u0435 \u0438\u0433\u0440\u0430 \u0432\u043e\u0432\u0441\u0435, \u0430 \u043d\u0430\u0441\u0442\u043e\u044f\u0449\u0430\u044f \u0440\u0430\u0431\u043e\u0442\u0430, \u043e\u0434\u043d\u043e\u043e\u0431\u0440\u0430\u0437\u043d\u0430\u044f \u0441\u043a\u0443\u0447\u043d\u0430\u044f \u0437\u0430\u0447\u0438\u0441\u0442\u043a\u0430 \u0442\u0430\u0440\u0435\u043b\u043e\u043a, \u0438 \u0432\u044b\u043a\u043e\u0432\u044b\u0440\u0438\u0430\u043d\u0438\u0435 \u0438\u0437 \u043d\u0438\u0445 \u043f\u0440\u0438\u0448\u0435\u043b\u044c\u0446\u0435\u0432. \u041e\u0441\u043e\u0431\u0435\u043d\u043d\u043e \u0431\u0435\u0441\u044f\u0442 \u0441\u0438\u0442\u0443\u0430\u0446\u0438\u0438, \u043a\u043e\u0433\u0434\u0430 \u043e\u0441\u0442\u0430\u0435\u0442\u0441\u044f \u043e\u0434\u0438\u043d \u0437\u0435\u043b\u0435\u043d\u044b\u0439 \u0447\u0435\u043b\u043e\u0432\u0435\u0447\u0435\u043a \u043d\u0430 \u043a\u0430\u0440\u0442\u0435, \u043d\u043e \u0442\u044b \u043d\u0435 \u0437\u043d\u0430\u0435\u0448\u044c \u0433\u0434\u0435, \u0438 \u043c\u0438\u0441\u0441\u0438\u044f \u043d\u0435 \u0437\u0430\u043a\u0430\u043d\u0447\u0438\u0432\u0430\u0435\u0442\u0441\u044f. \u041d\u043e \u043e\u0441\u043d\u043e\u0432\u043d\u0430\u044f \u043f\u0440\u0435\u0442\u0435\u043d\u0437\u0438\u044f \u0434\u0430\u0436\u0435 \u043d\u0435 \u043a \u043c\u0435\u0434\u0438\u0442\u0430\u0442\u0438\u0432\u043d\u043e\u043c\u0443 \u0433\u0435\u0439\u043c\u043f\u043b\u0435\u044e, \u0441 \u043d\u0430\u0448\u0443\u043c\u0435\u0432\u0448\u0438\u043c\u0438 95% = \u043f\u0440\u043e\u043c\u0430\u0445, \u0438 \u043d\u0435 \u043a \u043c\u0438\u043a\u0440\u043e\u043c\u0435\u043d\u0435\u0434\u0436\u043c\u0435\u043d\u0442\u0443 \u0438\u043d\u0432\u0435\u0440\u0442\u0430\u0440\u044f \u043a\u0430\u0436\u0434\u043e\u0433\u043e \u0441\u043e\u043b\u0434\u0430\u0442\u0438\u043a\u0430, \u0430 \u043a \u0447\u0443\u0434\u043e\u0432\u0438\u0449\u043d\u043e \u043c\u0430\u043b\u043e\u043c\u0443 \u043a\u043e\u043b\u0438\u0447\u0435\u0441\u0442\u0432\u0443 \u043a\u043e\u043d\u0442\u0435\u043d\u0442\u0430 \u043d\u0430 \u0442\u0430\u043a\u043e\u0439 \u0433\u0438\u0433\u0430\u043d\u0442\u0441\u043a\u0438\u0439 \u0445\u0440\u043e\u043d\u043e\u043c\u0435\u0442\u0440\u0430\u0436 \u0434\u043b\u044f \u043f\u043e\u043b\u043d\u043e\u0433\u043e \u043f\u0440\u043e\u0445\u043e\u0436\u0434\u0435\u043d\u0438\u044f. \u041f\u0440\u0438\u0447\u0435\u043c \u0441\u043d\u0430\u0447\u0430\u043b\u0430 \u0432\u0441\u0435 \u0438\u0434\u0435\u0442 \u043d\u043e\u0440\u043c\u0430\u043b\u044c\u043d\u043e \u0438 \u0431\u043e\u0434\u0440\u043e, \u043f\u043e\u044f\u0432\u043b\u044f\u044e\u0442\u0441\u044f \u043d\u043e\u0432\u044b\u0435 \u0437\u0430\u0434\u0430\u0447\u0438, \u043f\u0435\u0440\u0432\u044b\u0435 \u0447\u0430\u0441\u043e\u0432 10 \u0438\u043d\u0442\u0435\u0440\u0435\u0441\u043d\u043e \u0438\u0445 \u0434\u0435\u043b\u0430\u0442\u044c, \u0430 \u043f\u043e\u0442\u043e\u043c \u043f\u043e\u044f\u0432\u043b\u044f\u0435\u0442\u0441\u044f \u043c\u0438\u0441\u0441\u0438\u044f: \"\u0417\u0430\u0445\u0432\u0430\u0442\u0438 \u043b\u0438\u0434\u0435\u0440\u0430 \u043f\u0440\u0438\u0448\u0435\u043b\u044c\u0446\u0435\u0432\" \u0438 \u0432\u0441\u0435, \u043f\u043e\u0448\u0435\u043b \u0442\u044b \u043da\u0445\u0435\u0440, \u0438\u0449\u0438 \u0435\u0433\u043e \u043a\u0430\u043a \u0432\u0435\u0442\u0440\u0430 \u0432 \u043f\u043e\u043b\u0435. \n\n\u041c\u0435\u0441\u044f\u0446\u044b \u043f\u0440\u043e\u0445\u043e\u0434\u044f\u0442, \u0432\u043e\u0435\u043d\u043d\u044b\u0435 \u0431\u044e\u0434\u0436\u0435\u0442\u044b \u0440\u0430\u0441\u0442\u0443\u0442, \u0443\u043b\u0443\u0447\u0448\u0430\u044e\u0442\u0441\u044f \u043a\u043e\u0440\u0430\u0431\u043b\u0438\u043a\u0438, \u0431\u0440\u043e\u043d\u044c\u043a\u0438, \u043f\u0443\u0448\u043a\u0438, \u0430 \u0432\u043e\u0437 \u0438 \u043d\u044b\u043d\u0435 \u0442\u0430\u043c. \u0420\u0430\u0437\u0432\u043b\u0435\u043a\u0430\u0439 \u0441\u0435\u0431\u044f \u0441\u0430\u043c \u043f\u043e\u043a\u0430 \u043b\u0438\u0434\u0435\u0440 \u043d\u0435 \u0441\u043e\u0438\u0437\u0432\u043e\u043b\u0438\u0442 \u043f\u0440\u0438\u043b\u0435\u0442\u0435\u0442\u044c. \u0418 \u043f\u0440\u0438 \u0442\u043e\u043c, \u0445\u043e\u0442\u044c \u0431\u044b \u0432\u0438\u0434\u043e\u0432 \u043e\u043f\u0435\u0440\u0430\u0446\u0438\u0439 \u0431\u044b\u043b\u043e, \u043d\u0443 2 \u0438\u043b\u0438 3, \u0442\u0430\u043a \u043d\u0435\u0442, \u0432\u0441\u0435 \u043e\u043d\u0438 \u043e\u0434\u0438\u043d\u0430\u043a\u043e\u0432\u044b\u0435, \u0432\u044b\u0441\u0430\u0434\u0438\u0441\u044c \u0443\u0431\u0435\u0439 \u043f\u0440\u0438\u0448\u0435\u043b\u044c\u0446\u0435\u0432. \u041d\u0438 \u0442\u0435\u0431\u0435 \u0431\u043e\u043c\u0431\u0443 \u0440\u0430\u0437\u043c\u0438\u043d\u0438\u0440\u0443\u0439, \u043d\u0438 \u0442\u0435\u0431\u0435 \u0441\u043f\u0430\u0441\u0438 \u0437\u0430\u043b\u043e\u0436\u043d\u0438\u043a\u043e\u0432, \u0437\u0430\u0445\u0432\u0430\u0442\u0430 \u0444\u043b\u0430\u0433\u0430 \u043d\u0435\u0442, \u043f\u0440\u043e\u0441\u0442\u043e \u0432\u043e\u0442 \u0442\u0435\u0431\u0435 \u0432\u0438\u043b\u043a\u0430, \u0447\u0438\u0441\u0442\u0438. \u0415\u0441\u0442\u044c \u043a\u043e\u043d\u0435\u0447\u043d\u043e \u0437\u0430\u0449\u0438\u0442\u0430 \u0438 \u043d\u0430\u043f\u0430\u0434\u0435\u043d\u0438\u0435 \u043d\u0430 \u0431\u0430\u0437\u0443 \u043f\u0440\u0438\u0448\u0435\u043b\u044c\u0446\u0435\u0432, \u043d\u043e \u0442\u0430\u043c \u043c\u0435\u043d\u044f\u044e\u0442\u0441\u044f \u043f\u043e \u0441\u0443\u0442\u0438 \u0442\u043e\u043b\u044c\u043a\u043e \u0434\u0435\u043a\u043e\u0440\u0430\u0446\u0438\u0438. \u0421\u043a\u0443\u0447\u0438\u0449\u0430 \u043d\u0435\u0440\u0435\u0430\u043b\u044c\u043d\u0430\u044f, \u0434\u0430\u0436\u0435 \u043b\u0430\u043d\u0448\u0430\u0444\u0442 \u0443 \u043a\u0430\u0440\u0442 \u0438\u043c\u0435\u0435\u0442 \u043c\u0430\u043a\u0441\u0438\u043c\u0443\u043c 5 \u0432\u0430\u0440\u0438\u0430\u043d\u0442\u043e\u0432, \u0448\u043e \u043e\u0447\u0435\u043d\u044c \u043c\u0430\u043b\u043e, \u0431\u043e\u043b\u044c\u0448\u0438\u043d\u0441\u0442\u0432\u043e \u043a\u0430\u0440\u0442 - \u0441\u0442\u0430\u043d\u0434\u0430\u0440\u0442\u043d\u044b\u0435 \u0440\u0430\u043d\u0434\u043e\u043c\u043d\u043e \u043d\u0430\u0442\u044b\u043a\u0430\u043d\u043d\u044b\u0435 \u0430\u0441\u0441\u0435\u0442\u044b \u0441\u043a\u043b\u0430\u0434\u043e\u0432 \u0438 \u043e\u0444\u0438\u0441\u043e\u0432, \u043a\u043e\u0442\u043e\u0440\u044b\u0435 \u043f\u0440\u0438\u0435\u0434\u0430\u044e\u0442\u0441\u044f \u0443\u0436\u0435 \u043d\u0430 \u043f\u0435\u0440\u0432\u044b\u0445 15 \u0447\u0430\u0441\u0430\u0445. \n\n\u0410 \u0440\u0430\u0437\u0440\u0430\u0431\u044b \u0432\u0438\u0434\u0438\u043c\u043e \u0434\u0443\u043c\u0430\u043b\u0438, \u0447\u0442\u043e \u044d\u0442\u043e\u0433\u043e \u0445\u0432\u0430\u0442\u0438\u0442 \u0447\u0442\u043e\u0431\u044b \u0434\u0435\u0440\u0436\u0430\u0442\u044c \u0438\u0433\u0440\u043e\u043a\u0430 \u0437\u0430\u0438\u043d\u0442\u0435\u0440\u0435\u0441\u043e\u0432\u0430\u043d\u043d\u044b\u043c 50-60-100 \u0447\u0430\u0441\u043e\u0432, \u043d\u0443-\u043d\u0443. \u041f\u0440\u0435\u0436\u0434\u0435 \u0447\u0435\u043c \u0432\u044b\u043f\u0443\u0441\u043a\u0430\u0442\u044c \u043d\u043e\u0432\u0443\u044e \u0447\u0430\u0441\u0442\u044c, \u0432\u044b \u043b\u0438\u0431\u043e \u0432 \u044d\u0442\u043e\u0439 \u043a\u043e\u043d\u0442\u0435\u043d\u0442\u0430 \u0434\u043e\u0432\u0430\u043b\u0438\u0442\u0435, \u043b\u0438\u0431\u043e \u043a\u0440\u0435\u0441\u0442\u0438\u043a \u0441\u043d\u0438\u043c\u0438\u0442\u0435. \u041d\u0443 \u0438\u043b\u0438 \u0441\u043e\u043a\u0440\u0430\u0442\u0438\u0442\u0435 \u043f\u043e\u043b\u043d\u043e\u0435 \u043f\u0440\u043e\u0445\u043e\u0436\u0434\u0435\u043d\u0438\u0435 \u0447\u0430\u0441\u043e\u0432 \u0434\u043e 25. \u041a\u0442\u043e-\u0442\u043e \u0441\u043a\u0430\u0436\u0435\u0442, \u0442\u0430\u043a \u0441\u0431\u0438\u0432\u0430\u0439 \u0442\u0430\u0440\u0435\u043b\u043a\u0438, \u043a\u0440\u0443\u0442\u0438 \u0432\u0440\u0435\u043c\u044f, \u0437\u0430\u0447\u0435\u043c \u043b\u0435\u0442\u0430\u0442\u044c \u043d\u0430 \u0432\u0441\u0435 \u043e\u043f\u0435\u0440\u0430\u0446\u0438\u0438? \u041f\u043e\u0434 \u043a\u043e\u043d\u0435\u0446 \u044f \u0443\u0436\u0435 \u0438 \u043d\u0430\u0447\u0430\u043b \u0442\u0430\u043a \u0434\u0435\u043b\u0430\u0442\u044c, \u043d\u043e \u0432\u0441\u0435 \u0440\u0430\u0432\u043d\u043e, \u0442\u043e \u043d\u0430 \u0431\u0430\u0437\u0443 \u043d\u0430\u043f\u0430\u0434\u0443\u0442, \u0442\u043e \u0430\u0442\u0430\u043a\u0430 \u043d\u0430 \u0433\u043e\u0440\u043e\u0434, \u0442\u043e \u0434\u0443\u043c\u0430\u0435\u0448\u044c \u0432\u0434\u0440\u0443\u0433 \u0432 \u044d\u0442\u043e\u043c \u0431\u043e\u043b\u044c\u0448\u043e\u043c \u043a\u0440\u0435\u0439\u0441\u0435\u0440\u0435 \u043b\u0438\u0434\u0435\u0440 \u0438 \u043b\u0435\u0442\u0438\u0448\u044c \u0447\u0438\u0441\u0442\u0438\u0442\u044c, \u0430 \u0435\u0433\u043e \u043d\u0435\u0442 \u0438 \u043d\u0435\u0442. \n\n\u0412 \u043e\u0441\u0442\u0430\u043b\u044c\u043d\u043e\u043c \u0438\u0433\u0440\u0430 \u0432\u043f\u043e\u043b\u043d\u0435 \u043f\u0440\u0438\u044f\u0442\u043d\u0430\u044f \u0432 \u0434\u0443\u0445\u0435 \u043a\u043b\u0430\u0441\u0441\u0438\u043a\u0438 \u0442\u0430\u043a\u0442\u0438\u0447\u0435\u0441\u043a\u0438\u0445 \u043f\u0430\u0440\u0442\u0438\u0439\u043d\u044b\u0445 \u043f\u043e\u0448\u0430\u0433\u043e\u0432\u044b\u0445 \u0441\u0442\u0440\u0430\u0442\u0435\u0433\u0438\u0439, \u0438 \u0432\u0438\u0437\u0443\u0430\u043b\u044c\u043d\u044b\u0439 \u0441\u0442\u0438\u043b\u044c \u0445\u043e\u0442\u044c \u0438 \u0441\u0445\u0435\u043c\u0430\u0442\u0438\u0447\u0435\u0441\u043a\u0438\u0439, \u043d\u043e \u0437\u0430\u0442\u043e \u0438\u043d\u0442\u0443\u0438\u0442\u0438\u0432\u043d\u043e \u043f\u043e\u043d\u044f\u0442\u043d\u044b\u0439. \u041f\u0440\u0435\u0442\u0435\u043d\u0437\u0438\u0439 \u043a \u0433\u0435\u0439\u043c\u043f\u043b\u0435\u044e \u043d\u0435\u0442, \u0435\u0441\u043b\u0438 \u0432 \u0442\u0430\u043a\u043e\u043c \u0436\u0430\u043d\u0440\u0435 \u043d\u0435\u0442 \u043f\u0440\u043e\u043c\u0430\u0445\u043e\u0432 \u043d\u0430 95% \u0442\u043e \u044d\u0442\u043e \u0443\u0436\u0435 \u0438 \u043d\u0435 \u0438\u0433\u0440\u0430, \u0430 \u043c\u0438\u043a\u0440\u043e\u043c\u0435\u043d\u044d\u0434\u0436\u043c\u0435\u043d\u0442 \u044d\u0442\u043e \u043e\u0431\u044f\u0437\u0430\u0442\u0435\u043b\u044c\u043d\u044b\u0439 \u0430\u0442\u0440\u0438\u0431\u0443\u0442 \u0442\u0430\u043a\u043e\u0433\u043e \u0432\u0440\u0435\u043c\u044f\u043f\u0440\u043e\u0432\u043e\u0436\u0434\u0435\u043d\u0438\u044f. \u0415\u0441\u043b\u0438 \u0432\u0430\u043c \u043d\u0435 \u043f\u0440\u0438\u043d\u0446\u0438\u043f\u0438\u0430\u043b\u044c\u043d\u043e \u043f\u043e\u043b\u043d\u043e\u0435 \u043f\u0440\u043e\u0445\u043e\u0436\u0434\u0435\u043d\u0438\u0435 \u0442\u043e, \u043f\u043e\u0438\u0433\u0440\u0430\u0442\u044c \u0447\u0430\u0441\u043e\u0432 15 \u043c\u043e\u0436\u043d\u043e, \u0430 \u0434\u0430\u043b\u044c\u0448\u0435 \u0442\u043e\u043b\u044c\u043a\u043e \u0435\u0441\u043b\u0438 \u0442\u044b \u043a\u043e\u043b\u043e\u0441 \u043c\u044b\u0441\u043b\u0438 \u0432\u0435\u0440\u0445\u043e\u043c \u043d\u0430 \u0441\u0440a\u043b\u044c\u043d\u043e\u043c \u0432\u0435\u0434\u0440\u0435 \u0438\u043b\u0438 \u0442\u0430\u0440\u0435\u043b\u043e\u0447\u043d\u0438\u0446\u0430.", "timestamp_created": 1735649769, "timestamp_updated": 1735649769, "voted_up": false, "votes_up": 5, "votes_funny": 0, "weighted_vote_score": "0.503439366817474365", "comment_count": 0, 